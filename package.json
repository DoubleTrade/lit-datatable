{
  "name": "@doubletrade/lit-datatable",
  "version": "2.0.0",
  "description": "lit-datatable is a material design implementation of a data table, powered by lit-element.",
  "main": "lit-datatable.js",
  "scripts": {
    "lint": "eslint . --ext js,ts --ignore-path .gitignore --quiet",
    "prepublishOnly": "npm run lint && npm run build",
    "serve": "wds --node-resolve --app-index demo/index.html",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "wtr",
    "test:watch": "wtr --watch"
  },
  "files": [
    "*.js",
    "**/*.js",
    "*.d.ts",
    "**/*.d.ts",
    "**/*.js.map"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@doubletrade/lit-datepicker": "1.0.0",
    "@polymer/iron-demo-helpers": "^3.1.0",
    "@polymer/iron-flex-layout": "^3.0.1",
    "@polymer/iron-icon": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/paper-dropdown-menu": "^3.2.0",
    "@polymer/paper-icon-button": "^3.0.2",
    "@polymer/paper-input": "^3.2.1",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/paper-listbox": "^3.0.1",
    "@polymer/paper-tooltip": "^3.0.1",
    "@polymer/polymer": "^3.4.1",
    "fast-equals": "^2.0.0",
    "lit": "2.0.0-rc.2"
  },
  "devDependencies": {
    "@changesets/cli": "2.14.1",
    "@open-wc/testing": "^2.5.32",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "@web/dev-server": "0.1.8",
    "@web/test-runner": "^0.12.17",
    "@web/test-runner-saucelabs": "^0.5.0",
    "eslint": "7.22.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-html": "6.1.2",
    "eslint-plugin-import": "2.22.1",
    "npm-check-updates": "^11.3.0",
    "sinon": "^9.2.4",
    "typescript": "^4.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DoubleTrade/lit-datatable"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  }
